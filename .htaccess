RewriteEngine On
RewriteBase /

Options -Indexes

# Redirect to controllers
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule .* /index.php [L,QSA]

